@{
    ViewBag.Title = "SendCVConfirmedEs";
    Layout = "~/Views/Shared/_LayoutEs.cshtml";
}

<main>
    <section class="breadcrumbs">
        <div class="container">
            <div class="d-flex justify-content-between align-items-center">
                <h2>CV ENVIADO CORRECTAMENTE</h2>
                <ol>
                    <li><a href="@Url.Action("IndexEs","Home")">HOME</a></li>
                    <li><a href="@Url.Action("IndexEs","BolsaTrabajo")">ENVÍA TU CV</a></li>
                    <li>C.V.</li>
                </ol>
            </div>
        </div>
    </section>
    <section class="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12" data-aos="fade-up" data-aos-delay="300">
                    <br />
                    <div class="text-center"><h4>Gracias por su mensaje!</h4></div>
                    <br />
                    <div class="text-center"><h5>Su C.V. ha sido enviado correctamente.</h5></div>
                    <br /><br />
                    <p class="text-center">
                        <a href="@Url.Action("IndexEs", "BolsaTrabajo")" class="btn_Send">OK</a>
                    </p>
                    <br />
                </div>
            </div>
        </div>
    </section>
</main>
